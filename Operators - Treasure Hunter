import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int ben = (a*b)/100;
        int a1 = a-ben;
        int bb = (a1*c)/100;
        int a2 = a1-bb;
        int p3 = a2/3;
        System.out.println(ben);
        System.out.println(bb);
        System.out.println(p3);
    }
}
