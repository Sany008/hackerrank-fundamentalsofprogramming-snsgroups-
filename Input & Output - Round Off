import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        float n = sc.nextFloat();
        System.out.println(Math.round(Math.floor(n)));
        System.out.println(Math.round(Math.ceil(n)));
        System.out.println(Math.round(Math.floor(n)));
    }
}
