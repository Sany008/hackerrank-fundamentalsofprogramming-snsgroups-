import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        double c = (Math.sqrt(Math.pow(a-3,2)+Math.pow(b-4,2)));
        System.out.println((int)c);
    }
}
