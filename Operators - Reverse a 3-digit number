import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int reverse = 0;
        for (;a!=0;){
            int r = a%10;
            reverse = (reverse*10)+r;
            a=a/10;
        }
        System.out.println(reverse);
    }
}
